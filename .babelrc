{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "node": 6
        }
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "add-module-exports",
    ["@babel/plugin-transform-modules-commonjs", { "lazy": true }]
  ]
}
